<aw:query cmd="hasPrivilege" privilege="writer">
  <aw:is name="hasPrivilege">
    <aw:query cmd="singleton">
      <aw:nis name="singleton" aw:newvar.url="activeTemplateDirectory">
        <aw:query cmd="append" name="url" value="/rename.html" />
        <aw:query cmd="urlAppend" name="rolonUuid" aw:value="rolonUuid"/>
        <a aw:href="url"><span style="color:green; font-style:italic">rename</span></a>
      </aw:nis>
    </aw:query>
  </aw:is>
</aw:query>
